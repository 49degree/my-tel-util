<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent" android:background="@color/white">
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="35dip">
		<TextView android:layout_width="wrap_content"
			android:layout_height="fill_parent" style="@style/apsai_dialog_text_style"
			android:text="@string/apsai_sale_order_card_code" android:gravity="center_vertical|center_horizontal"
			android:layout_weight="4" />
		<TextView android:layout_width="wrap_content"
			android:layout_height="fill_parent" style="@style/apsai_dialog_text_style"
			android:text="@string/apsai_sale_order_planno" android:gravity="center_vertical|center_horizontal"
			android:layout_weight="4" />
		<TextView android:layout_width="wrap_content"
			android:layout_height="fill_parent" android:text="@string/apsai_sale_order_card_name"
			android:gravity="center_vertical|center_horizontal" style="@style/apsai_dialog_text_style"
			android:layout_weight="2" />
	</LinearLayout>
	<LinearLayout android:orientation="horizontal"
		android:layout_width="fill_parent" android:layout_height="1dip"
		android:background="@color/black" />
	<ListView android:id="@+id/insuinfo_list"
		android:layout_width="fill_parent" android:layout_height="fill_parent"
		android:cacheColorHint="@color/white" android:scrollbars="vertical" />
</LinearLayout>