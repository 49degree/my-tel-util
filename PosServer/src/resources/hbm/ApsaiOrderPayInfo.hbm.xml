<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.a3650.posserver.core.bean.ApsaiOrderPayInfo" table="ApsaiOrderPayInfo">
	<id name="payId" column="payId" type="long">
		<generator class="assigned"></generator>
	</id>
	<property name="apsaiId" column="apsaiId" type="string"></property>
	<property name="payResult" column="payResult" type="char"></property>
	<property name="payAmount" column="payAmount" type="long"></property>
	<property name="payTime" column="payTime" type="string"></property>
</class>
</hibernate-mapping>