		android:dividerHeight="1.0dip" android:layout_marginRight="65.0dip"
		android:layout_marginLeft="65.0dip" android:divider="#BEC8D1"
		android:drawSelectorOnTop="false" android:fadingEdge="none"
		android:layout_weight="1.0">
	</ListView>
	<LinearLayout android:gravity="center_vertical"
		android:layout_width="wrap_content" android:layout_gravity="center_horizontal"
