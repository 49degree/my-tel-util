<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<LinearLayout android:layout_width="fill_parent"
		android:orientation="horizontal" android:layout_marginRight="62.0dip"
		android:background="@drawable/table_head" android:gravity="center_vertical"
		android:layout_marginLeft="62.0dip" android:layout_height="58.0dip"
		android:layout_marginTop="40.0dip">
		<ImageView android:src="@drawable/prompt"
			android:layout_marginLeft="30.0dip" android:layout_width="wrap_content"
			android:layout_height="wrap_content" />
		<TextView android:text="未通过原因" android:gravity="center"
			android:layout_marginLeft="8.0dip" android:layout_height="fill_parent"
			android:layout_width="wrap_content" android:textColor="@color/tell_normal"
			android:textSize="24px" />
	</LinearLayout>


	<ListView android:id="@android:id/list" android:layout_width="fill_parent"
